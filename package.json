{
  "name": "@leano_z/convert-dayjs",
  "version": "0.0.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:es": "tsc -b ./tsconfig.esm.json",
    "build:types": "tsc -b ./tsconfig.types.json",
    "preview": "vite preview",
    "test": "vitest",
    "pub": "npm publish --access public"
  },
  "license": "ISC",
  "files": [
    "dist"
  ],
  "main": "./dist/convert-dayjs.umd.cjs",
  "module": "./dist/convert-dayjs.js",
  "types": "./dist/types/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/convert-dayjs.js",
      "require": "./dist/convert-dayjs.umd.cjs"
    }
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "dayjs": "^1.11.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wulakaka/-leano_z-convert-dayjs.git"
  }
}
